<p id="notice"><%= notice %></p>

<h1>Ithaca Community Gardens - Admin Panel</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>Committee 1</th>
      <th>Committee 2</th>
      <th>Committee 3</th>
      <th>Committee 4</th>
      <th>Committee 5</th>
      <th>Workday 1</th>
      <th>Workday 2</th>
      <th>Workday 3</th>
      <th>Workday 4</th>
      <th>Workday 5</th>
      <th>Workday 6</th>
      <th>Workday 7</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meta.each do |metum| %>
      <tr>
        <td><%= metum.username %></td>
        <td>****</td>
        <td><%= metum.committee1 %></td>
        <td><%= metum.committee2 %></td>
        <td><%= metum.committee3 %></td>
        <td><%= metum.committee4 %></td>
        <td><%= metum.committee5 %></td>
        <td><%= metum.workday1 %></td>
        <td><%= metum.workday2 %></td>
        <td><%= metum.workday3 %></td>
        <td><%= metum.workday4 %></td>
        <td><%= metum.workday5 %></td>
        <td><%= metum.workday6 %></td>
        <td><%= metum.workday7 %></td>
        <td><%= link_to 'Edit', edit_metum_path(metum) %></td>
      </tr>
    <% end %>
  </tbody>

</table>

<%= link_to("Manage Users", :controller => "users", :action => "index") %>
   <br>
